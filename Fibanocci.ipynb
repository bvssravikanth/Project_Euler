    "def fibanocci():\n",
    "    a=1\n",
    "    b=2\n",
    "    fiblist = [a,b]\n",
    "    c=0\n",
    "    while(c<4000000):\n",
    "        c=a+b\n",
    "        fiblist.append(c)\n",
    "        a=b\n",
    "        b=c\n",
    "        \n",
    "    return fiblist"
   ]
  },
